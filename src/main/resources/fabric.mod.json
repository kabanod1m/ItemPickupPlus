{
  "schemaVersion": 1,
  "id": "itempickupplus",
  "version": "${version}",
  "name": "ItemPickupPlus",
  "description": "A Fabric Minecraft mod that lets you configure how you interact with dropped items",
  "authors": [
    "kabanod1m"
  ],
  "contact": {
    "website": "winglessbirds.org",
    "repo": "https://github.com/kabanod1m/ItemPickupPlus"
  },
  "license": "MIT",
  "icon": "assets/itempickupplus/icon.png",
  "environment": "*",
  "entrypoints": {
    "client": [
      "org.winglessbirds.itempickupplus.client.ItemPickupPlusClient"
    ],
    "main": [
      "org.winglessbirds.itempickupplus.ItemPickupPlus"
    ],
    "modmenu": [
      "org.winglessbirds.itempickupplus.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "itempickupplus.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "cloth-config2": ">=${cloth_config_version}"
  }
}
